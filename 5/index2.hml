<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
    let N = +prompt("Введите целое число N: ");
    if (isNaN(N)) {
        console.log("Ошибка: введено не число!");
    } else {
        let sum = 0;
        let i = 1;
        while (i <= N) {
            sum += i;
            i++;
        }
       console.log(`Сумма чисел от 1 до ${N} равна: ${sum}`);
    }
    </script>
</body>
</html>